{
  "name": "nextjs-template",
  "version": "2.0.0",
  "author": "Nishant Kohli",
  "description": "Next.js v16 template with Material-UI, docker, and Tailwind CSS",
  "private": true,
  "scripts": {
    "set-up": "sh setup.sh",
    "dev": "next dev -p 3000 --webpack",
    "build": "next build --webpack",
    "start": "next start",
    "prod": "pnpm build && pnpm start",
    "lint": "eslint --fix",
    "lint:check": "eslint",
    "prepare": "husky || true",
    "check-updates": "ncu --format group -u",
    "install-updates": "pnpm check-updates && pnpm install"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/material-nextjs": "^7.3.5",
    "next": "16.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@nish1896/eslint-flat-config": "^3.0.3",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.{json,html,css,sass,scss}": [
      "prettier --write --ignore-unknown"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "packageManager": "pnpm@10.20.0",
  "keywords": [
    "nextjs",
    "template",
    "material-ui",
    "tailwindcss",
    "docker"
  ],
  "license": "MIT"
}
